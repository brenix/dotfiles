Xft.dpi: 109
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

XTerm*utf8: 1
XTerm*eightBitInput: false
XTerm*eightBitControl: false
XTerm*eightBitOutput: true
XTerm*fullscreen: never
XTerm*omitTranslation: fullscreen

URxvt*loginShell: true
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*transparent: false
URxvt*cursorBlink: true
URxvt*urgentOnBell: true
URxvt*letterSpace: -2
URxvt*font: xft:Terminus:size=9

Xcursor.theme: Colloid-dark-cursors
Xcursor.size: 22

*background: #1e1e2e
*foreground: #cdd6f4
*cursorColor: #f5e0dc

! black
*color0: #45475a
*color8: #585b70

! red
*color1: #f38ba8
*color9: #f38ba8

! green
*color2: #a6e3a1
*color10: #a6e3a1

! yellow
*color3: #f9e2af
*color11: #f9e2af

! blue
*color4: #89b4fa
*color12: #89b4fa

! magenta
*color5: #f5c2e7
*color13: #f5c2e7

! cyan
*color6: #94e2d5
*color14: #94e2d5

! white
*color7: #bac2de
*color15: #a6adc8
